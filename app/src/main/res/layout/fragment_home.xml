<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingTop="?attr/actionBarSize"
                android:id="@+id/home_container">

    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:focusableInTouchMode="true">

        <LinearLayout
                android:id="@+id/line1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

            <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_weight="1">

                <ImageView
                        android:id="@+id/imageViewReceipt"
                        android:layout_width="match_parent"
                        android:layout_height="120dp"
                        android:src="@drawable/baseline_receipt_black_48"
                        android:padding="15dp"/>

                <TextView
                        android:id="@+id/textViewReceipt"
                        android:layout_width="match_parent"
                        android:layout_height="80dp"
                        android:text="@string/nav_receipt"
                        android:textSize="@dimen/text_size_25"
                        android:gravity="center_horizontal"
                        android:textStyle="bold"/>

            </LinearLayout>



            <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_weight="1">

                <ImageView
                        android:id="@+id/imageViewStorage"
                        android:layout_width="match_parent"
                        android:layout_height="120dp"
                        android:src="@drawable/factory_stock_house"
                        android:padding="15dp"/>

                <TextView
                        android:id="@+id/textViewStorage"
                        android:layout_width="match_parent"
                        android:layout_height="80dp"
                        android:text="@string/nav_storage"
                        android:textSize="@dimen/text_size_25"
                        android:textStyle="bold"
                        android:gravity="center_horizontal"/>

            </LinearLayout>

            <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_weight="1">

                <ImageView
                        android:id="@+id/imageViewMaterial"
                        android:layout_width="match_parent"
                        android:layout_height="120dp"
                        android:src="@drawable/baseline_unarchive_black_48"
                        android:padding="15dp"/>

                <TextView
                        android:id="@+id/textViewMaterial"
                        android:layout_width="match_parent"
                        android:layout_height="80dp"
                        android:text="@string/nav_material_issuing"
                        android:textSize="@dimen/text_size_25"
                        android:textStyle="bold"
                        android:gravity="center_horizontal"/>

            </LinearLayout>
        </LinearLayout>

        <LinearLayout
                android:id="@+id/line2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

            <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_weight="1">

                <ImageView
                        android:id="@+id/imageViewProperty"
                        android:layout_width="match_parent"
                        android:layout_height="120dp"
                        android:src="@drawable/baseline_monetization_on_black_48"
                        android:padding="15dp"/>

                <TextView
                        android:id="@+id/textViewProperty"
                        android:layout_width="match_parent"
                        android:layout_height="80dp"
                        android:text="@string/nav_property"
                        android:textSize="@dimen/text_size_25"
                        android:gravity="center_horizontal"
                        android:textStyle="bold"/>

            </LinearLayout>



            <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_weight="1">

                <ImageView
                        android:id="@+id/imageViewTagPrinter"
                        android:layout_width="match_parent"
                        android:layout_height="120dp"
                        android:src="@drawable/baseline_print_black_48"
                        android:padding="15dp"/>

                <TextView
                        android:id="@+id/textViewTagPrinter"
                        android:layout_width="match_parent"
                        android:layout_height="80dp"
                        android:text="@string/nav_printer"
                        android:textSize="@dimen/text_size_25"
                        android:gravity="center_horizontal"
                        android:textStyle="bold"/>

            </LinearLayout>

            <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:visibility="visible">

                <ImageView
                        android:id="@+id/imageViewLogout"
                        android:layout_width="match_parent"
                        android:layout_height="120dp"
                        android:src="@drawable/baseline_exit_to_app_black_48"
                        android:padding="15dp"/>

                <TextView
                        android:id="@+id/textViewLogout"
                        android:layout_width="match_parent"
                        android:layout_height="80dp"
                        android:text="@string/nav_logout"
                        android:textSize="@dimen/text_size_25"
                        android:textStyle="bold"
                        android:gravity="center_horizontal"/>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
                android:id="@+id/line3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
            <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:visibility="visible">

                <ImageView
                        android:id="@+id/imageViewSetting"
                        android:layout_width="match_parent"
                        android:layout_height="120dp"
                        android:src="@drawable/baseline_settings_black_48"
                        android:padding="15dp"/>

                <TextView
                        android:id="@+id/textViewSetting"
                        android:layout_width="match_parent"
                        android:layout_height="80dp"
                        android:text="@string/nav_setting"
                        android:textSize="@dimen/text_size_25"
                        android:textStyle="bold"
                        android:gravity="center_horizontal"/>

            </LinearLayout>

            <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:visibility="visible">

                <ImageView
                        android:id="@+id/imageViewGuest"
                        android:layout_width="match_parent"
                        android:layout_height="120dp"
                        android:src="@drawable/baseline_transfer_within_a_station_black_48"
                        android:padding="15dp"/>

                <TextView
                        android:id="@+id/textViewGuest"
                        android:layout_width="match_parent"
                        android:layout_height="80dp"
                        android:text="@string/nav_guest"
                        android:textSize="@dimen/text_size_25"
                        android:textStyle="bold"
                        android:gravity="center_horizontal"/>

            </LinearLayout>

            <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_weight="1">

                <ImageView
                        android:id="@+id/imageViewAbout"
                        android:layout_width="match_parent"
                        android:layout_height="120dp"
                        android:src="@drawable/baseline_info_black_48"
                        android:padding="15dp"/>

                <TextView
                        android:id="@+id/textViewAbout"
                        android:layout_width="match_parent"
                        android:layout_height="80dp"
                        android:text="@string/nav_about"
                        android:textSize="@dimen/text_size_25"
                        android:textStyle="bold"
                        android:gravity="center_horizontal"/>

            </LinearLayout>



        </LinearLayout>

    </LinearLayout>



</RelativeLayout>