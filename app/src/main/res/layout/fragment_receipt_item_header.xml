<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_height="wrap_content" android:orientation="horizontal" android:layout_width="fill_parent">

    <TextView android:text="header"
            android:id="@+id/outSourcedProcessLowerItemDetailHeader"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textStyle="bold"
            android:textColor="@color/md_black_1000"
            android:textSize="@dimen/text_size_30"
            android:gravity="center_vertical"
            android:paddingLeft="10dp"
            android:layout_weight="1">
    </TextView>

    <TextView android:text= "contentStatic"
            android:id="@+id/outSourcedProcessLowerItemDetailContentStatic"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textStyle="bold"
            android:textColor="@color/md_black_1000"
            android:textSize="@dimen/text_size_30"
            android:gravity="center"
            android:layout_weight="1">
    </TextView>

    <TextView android:text= "content"
            android:id="@+id/outSourcedProcessLowerItemDetailContentDynamic"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textStyle="bold"
            android:textColor="@color/md_black_1000"
            android:textSize="@dimen/text_size_30"
            android:gravity="center"
            android:layout_weight="1">
    </TextView>






</LinearLayout>